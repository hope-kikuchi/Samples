<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.javafx.MainController">
   <children>
      <Label alignment="CENTER" maxWidth="1.7976931348623157E308" style="-fx-padding: 10px;" text="白川郷">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <BorderPane fx:id="borderPane" VBox.vgrow="ALWAYS">
         <center>
            <ImageView fx:id="imageView" fitHeight="200.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</VBox>
